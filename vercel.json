{
  "version": 2,
  "builds": [
    {
      "src": "slip-reader/package.json",
      "use": "@vercel/static-build",
      "config": {
        "workDirectory": "slip-reader",
        "outputDirectory": "build"
      }
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}